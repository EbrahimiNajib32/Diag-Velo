{% extends 'base.html.twig' %}

{% block title %}
    Éléments de diagnostic pour {{ typeDiagnostic.nomType }}
{% endblock %}

{% block body %}


    <h1>Éléments de diagnostic pour {{ typeDiagnostic.nomType }}</h1>
    <ul>
        {% for element in elementsDiagnostic %}
            <li>ID de DiagnosticTypeElementcontrol : {{ element.id }}</li>
            <li>ID de l'élément de contrôle : {{ element.idElementcontrol.id }}</li>
            <li>Contenu de l'élément : {{ element.idElementcontrol.element }}</li>
        {% endfor %}
    </ul>
{% endblock %}
